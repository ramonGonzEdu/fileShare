doctype html
include listItem.pug
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet" href="/assets/style.css")
    title Files
  body 
    h1!= user
    .list
      each val in files
        +listItem("/files/"+user+"/"+val)= val
